{
  "name": "speedy-vision",
  "version": "0.8.3-wip",
  "description": "GPU-accelerated Computer Vision for JavaScript",
  "author": "Alexandre Martins <alemartf@gmail.com> (https://github.com/alemart)",
  "homepage": "https://github.com/alemart/speedy-vision-js",
  "license": "Apache-2.0",
  "main": "src/main.js",
  "types": "types/main.d.ts",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "build": "npm run build-dev && npm run build-pro && npm run build-dts",
    "build-dev": "NODE_ENV=development webpack --mode development",
    "build-pro": "NODE_ENV=production webpack --mode production",
    "build-dts": "tsc",
    "make": "cd src/core/wasm; make; cd ..",
    "watch": "webpack --watch"
  },
  "keywords": [
    "computer vision",
    "machine vision",
    "image processing",
    "linear algebra",
    "keypoint",
    "opencv",
    "gpgpu"
  ],
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.16.0",
    "raw-loader": "^4.0.1",
    "terser-webpack-plugin": "^2.3.6",
    "typescript": "^4.4.4",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {}
}
